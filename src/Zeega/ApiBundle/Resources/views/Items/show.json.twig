{
"items":
[
	{% if userIsAdmin is defined and user is defined %}
		{% include 'ZeegaApiBundle:Items:base.json.twig' with {'item': item, 'userIsAdmin': userIsAdmin, 'user': user} %}
	{% else %}
		{% include 'ZeegaApiBundle:Items:base.json.twig' with {'item': item} %}
	{% endif %}
],
	"request":
		{% if request is defined %}
			{{ request | json_encode | raw}}
		{% else %}
		{}
		{% endif %}
}