{
{% block header %}{% endblock %}
"{% block listName %}items{% endblock %}":
[
	{% for item in items %}
		{% include 'ZeegaApiBundle:Items:base.json.twig' with {'item': item} %}	
		{%- if loop.index0 < items|length - 1 -%}
		,
		{%- endif -%}
    {% endfor %}
],
"{% block totalCountName %}returned_items_count{% endblock %}":
	{% if items is defined %}
		{{ items|length }}
	{% else %}
		0
	{% endif %}	
,
"{% block countName %}items_count{% endblock %}":
	{% if items_count is defined %}
		{{ items_count }}
	{% else %}
		0
	{% endif %}
}