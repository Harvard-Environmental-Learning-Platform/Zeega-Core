{
{% block header %}{% endblock %}
"tags":
	{% if tags|length > 0 %}
		[
			{% set len = tags|length - 1 -%}
	
			{% for i in 0..len %}
			{
				"id" : "{{ tags[i].id }}",
				"tag_name" : "{{ tags[i].name }}",
				"tag_date" :"{{ tags[i].date_created is empty ? "" : tags[i].date_created | date("m/d/Y") }}"
			}
			{% if i < len %}
				,
			{% endif %}
			{% endfor %}
		],
	{% else %}
		{},
	{% endif %}	
"tags_count": 
	{% if tags is defined %}
		{{ tags|length }}
	{% else %}
		0
	{% endif %}	
}