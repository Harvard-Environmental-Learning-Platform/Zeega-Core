{% block title %}Dashboard{% endblock %}


{%  block stylesheets %}
	<style type="text/css">
	
		/*
			Background image code lives here, not in CSS, because this is specific to each page
			and could be made dynamic too (i.e. randomly picking a different image each load)
			quite easily. Exposing the code here should be helpful.
		*/
	
		html { 
		background: url(http://farm5.staticflickr.com/4096/4827587717_c362a1b42e_b.jpg) no-repeat center center fixed; 
		-webkit-background-size: cover;
		-moz-background-size: cover;
		-o-background-size: cover;
		background-size: cover;
		}
	
	</style>
	
	<link rel="stylesheet" href="{{ asset('css/civic-center-bootstrap.css') }}" type="text/css" media="screen" /> <!-- Including Bootstrap and our styles -->
	<link rel="stylesheet" href="{{ asset('css/zeega.community.css') }}" type="text/css" media="screen" /> <!-- Including Bootstrap and our styles -->
	
